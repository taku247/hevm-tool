# HyperEVM Testnet Environment Variables
# Copy this file to .env and fill in your actual values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Alternative private key variable name
TESTNET_PRIVATE_KEY=your_private_key_here

# HyperEVM Testnet RPC URL (optional, defaults to official RPC)
HYPERLIQUID_TESTNET_RPC=https://rpc.hyperliquid-testnet.xyz/evm

# Deployment settings
DEPLOY_GAS_LIMIT=2000000
DEPLOY_GAS_PRICE=1600000000